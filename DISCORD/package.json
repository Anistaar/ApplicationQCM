{
  "name": "text2quiz-discord-bot",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "TS_NODE_TRANSPILE_ONLY=1 node --env-file=.env --loader ts-node/esm src/index.ts",
